<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/panelCF"
    android:layout_width="650dp"
    android:layout_height="390dp"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="25dp">

    <EditText
        android:id="@+id/nombrefamiliaf"
        android:layout_width="308dp"
        android:layout_height="46dp"
        android:layout_alignLeft="@+id/textView1"
        android:layout_below="@+id/textView1"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.773"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
        app:layout_constraintVertical_bias="0.064" />


    <TextView
        android:id="@+id/textview112"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/passwordr"
        android:layout_centerVertical="true"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="8dp"
        android:text="Nombre de la Familia:"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.796"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/nombrefamiliaf"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
        app:layout_constraintVertical_bias="0.141" />


    <Button
        android:id="@+id/crearf"
        android:layout_width="415dp"
        android:layout_height="48dp"
        android:layout_alignRight="@+id/passwordr"
        android:layout_below="@+id/passwordr"
        android:layout_marginBottom="7dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:text="Crear familia"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.593"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        app:layout_constraintVertical_bias="1.0" />

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="650dp"
        android:layout_height="61dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:background="#FFFE9F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/nuevafamiliatitlte"
            android:layout_width="305dp"
            android:layout_height="64dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:gravity="center"
            android:lineSpacingExtra="10sp"
            android:text="NUEVA FAMILIA"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#4A86E8"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />
    </android.support.constraint.ConstraintLayout>

    <Button
        android:id="@+id/desdeDispositivo"
        android:layout_width="280dp"
        android:layout_height="46dp"
        android:layout_marginLeft="0dp"
        android:text="Cargar desde dispositivo"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="44dp"
        app:layout_constraintTop_toBottomOf="@+id/textview112"
        app:layout_constraintRight_toLeftOf="@+id/imagenvaciaf"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.476" />

    <Button
        android:id="@+id/button3"
        android:layout_width="280dp"
        android:layout_height="46dp"
        android:layout_marginLeft="0dp"
        android:text="cargar desde cÃ¡mara"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/imagenvaciaf"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="101dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/desdeDispositivo"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imagenvaciaf"
        android:layout_width="110dp"
        android:layout_height="113dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:scaleType="fitCenter"
        app:layout_constraintHorizontal_bias="0.738"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/imagenvacia"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/nombrefamiliaf"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/crearf"
        app:layout_constraintVertical_bias="0.479" />


</android.support.constraint.ConstraintLayout>